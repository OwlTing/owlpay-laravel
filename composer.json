{
    "name": "Owlting/owlpay-php",
    "description": "Owlpay SDK.",
    "type": "library",
    "authors": [
        {
            "name": "Eric",
            "email": "eric_liao@owlting.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Owlting\\OwlPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7",
        "guzzlehttp/guzzle": "^6.2",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.7 || ~3.7.8 || ~3.8.6 || ^4.8 || ^5.2 || ^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Owlting\\OwlPay\\Providers\\OwlPayServiceProvider"
            ],
            "facades": [
                "Owlting\\OwlPay\\Facades\\OwlPay"
            ]
        }
    }
}
